<div class="container">
<div class="panel devise-panel" style="font-size:15px ;min-height: 500px;">
  <div class="panel-heading text-center ">
    <a  class="sign-title">
      Search Result &nbsp;(<%=  @contents.size%>)
    </a>
  </div>
<% @contents.each do | content|%>
<div style="width: 100%;margin-top: 6px;height: 30px;padding-top: 7px" >
  <a style="float: left" href="/articles/<%=content.id %>"><%= content.title %></a>
  <p style="float: right;font-size:12px;color: #888;bottom: 0"><%=content.created_at %></p>
</div>
<%end %>

  </div>
</div>